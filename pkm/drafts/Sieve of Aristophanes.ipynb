   "source": [
    "# Sieve of Aristophanes\n",
    "\n",
    "Generates prime numbers up to a given limit"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def sieve(n):\n",
    "  not_prime = []\n",
    "  for i in range(2, n + 1):\n",
    "    if i not in not_prime:\n",
    "      print(i)\n",
    "      for j in range(i * i, n + 1, i):\n",
    "        not_prime.append(j)"
   ]
  },
